/*Estilo boton validacion ingresos*/
#validarIngFin{
	float: right;
	margin-top: -6px;
}

/*Estilo para barra flotante de navegacion*/
.navCMIng {
	position: fixed;
	transition: all 0.3s linear;
	box-shadow: 2px 2px 8px 0px rgba(0, 0, 0, .4);
	top: 185px;
}

.navCMIng ul {
	margin-top: 0;
	margin-bottom: 0rem;
	list-style-type: none;
	padding: 0;
}

.navCMIng li {
	height: 45px;
	position: relative;
	width: 50px;
}

.navCMIng li a {
	color: #fff !important;
	display: block;
	height: 100%;
	width: 100%;
	line-height: 45px;
	padding-left: 25%;
	border-bottom: 1px solid rgba(0, 0, 0, .4);
	transition: all .3s linear;
	text-decoration: none !important;
}

.navCMIng li:nth-child(1) a {
	background: #4267B2;
}

.navCMIng .fa-scale-balanced {
	position: absolute;
	top: 10px;
	color: white;
}

.navCMIng ul li a span {
	display: none;
	font-weight: bold;
	letter-spacing: 1px;
}

.navCMIng a:hover {
	z-index: 1;
	width: 400px;
	border-bottom: 1px solid rgba(0, 0, 0, .5);
	box-shadow: 0 0 1px 1px rgba(0, 0, 0, .3);
}

.navCMIng ul li:hover a span {
	padding-left: 10%;
	display: block;
	font-size: 15px;
}

