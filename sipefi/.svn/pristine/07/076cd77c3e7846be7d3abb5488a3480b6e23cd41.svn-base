				    let valorTI = $(data).find(".opcTipoIngRIC").val();
				    let valorMP = $(data).find(".opcMecPagoRIC").val();
				    let valorPI = $(data).find(".valPorIng").val();
				    let valorPE = $(data).find(".valPorEtiq").val();
				    objTIMP.push([valorTI,valorMP,valorPI,valorPE]);
